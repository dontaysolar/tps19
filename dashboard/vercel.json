{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@dashboard-api-url",
    "NEXT_PUBLIC_WS_URL": "@dashboard-ws-url",
    "ORGANISM_API_URL": "@organism-api-url"
  },
  "rewrites": [
    {
      "source": "/api/organism/:path*",
      "destination": "https://your-organism-api.com/api/:path*"
    }
  ]
}
