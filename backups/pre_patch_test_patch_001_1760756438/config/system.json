{
    "system": {
        "name": "TPS19",
        "version": "1.0.0",
        "environment": "production",
        "debug": false
    },
    "database": {
        "path": "/opt/tps19/data/databases/",
        "backup_interval": 3600,
        "max_connections": 10
    },
    "logging": {
        "level": "INFO",
        "file": "/opt/tps19/logs/system.log",
        "max_size": "10MB",
        "backup_count": 5
    },
    "security": {
        "encryption": true,
        "mfa_required": true,
        "session_timeout": 3600
    }
}
