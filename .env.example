# TPS19 Environment Configuration
# Copy this file to .env and fill in your API keys

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Alpha Vantage API
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Google Sheets (use service account JSON file)
GOOGLE_APPLICATION_CREDENTIALS=/workspace/config/google_credentials.json

# System Configuration
TPS19_ENV=production
TPS19_LOG_LEVEL=INFO
TPS19_DB_PATH=/opt/tps19/data/databases/

# Trading Configuration
DEFAULT_TRADING_PAIR=BTC_USDT
MAX_POSITION_SIZE=0.1
RISK_PER_TRADE=0.02